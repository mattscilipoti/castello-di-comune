[alias]
	ci = commit
  c = commit -m
  a = add
  di = diff
  dic = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
	st = status
  out = log origin..HEAD
	svnpull = svn rebase
	svnpush = svn dcommit

